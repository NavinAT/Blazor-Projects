@page "/counter"

<h1>Counter</h1>

<p>Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>


@code {

    public string DataMember { get; set; } = "EDInsurance";
    int currentCount;

    void IncrementCount()
    {
        currentCount++;
    }

}